          <CharacterEffectData LeftMove="x" LeftTime="x" LeftFade="x" LeftReset="x" MiddleMove="x" MiddleTime="x" MiddleFade="x" MiddleReset="x" RightMove="x" RightTime="x" RightFade="x" RightReset="x"/>
